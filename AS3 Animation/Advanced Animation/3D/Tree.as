package {	import flash.display.Sprite;		public class Tree extends Sprite {		internal var xpos:Number;		internal var ypos:Number;		public var zpos:Number;				public function Tree():void {			init();		}				private function init():void {			graphics.lineStyle(0, 0xffffff);			graphics.lineTo(0, -140 - Math.random() * 20);			graphics.moveTo(0, -30 - Math.random() * 30);			graphics.lineTo(Math.random() * 80 - 40, -100 - Math.random() * 40);			graphics.moveTo(0, -60 - Math.random() * 40);			graphics.lineTo(Math.random()*60 - 30, -110 - Math.random() * 20);		}	}}